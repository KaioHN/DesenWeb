const PagContato = {
    template: `
      <div>
        <h1>Contato</h1>
        <p>Entre em contato conosco através do formulário abaixo:</p>
        <form>
          <label for="nome">Seu Nome:</label>
          <input type="text" id="nome" name="nome">
          <label for="email">Seu e-mail para contato:</label>
          <input type="text" id="email" name="email">
  
          <label for="mensagem">Informe seu problema:</label>
          <textarea id="mensagem" name="mensagem"></textarea>
  
          <button type="button" @click="click">Enviar relatório</button>
        </form>
      </div>
    `,
    methods: {
        click() {
            alert('Um e-mail com informações adicionais para o seu problema foi enviado para sua caixa de entrada! Verifique sua caixa de spam caso não apareça')
        }
    },
};
